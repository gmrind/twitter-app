<% provide(:title, "All Users") %>
<h1>All Users</h1>

<%= will_paginate %>

<ul class="users">
	<% @users.each do |user| %>
	<% if user.fullname !=current_user.fullname %>
		<li>
			<%= link_to avatar(user), user %>
			<%= link_to user.fullname, user %>
		</li>
		<% end %>
<% end %>	
</ul>

<%= will_paginate %>
